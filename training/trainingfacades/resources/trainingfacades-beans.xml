<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;de.hybris.training.facades.product.data.GenderData>"/>

	</bean>

	<bean class="de.hybris.platform.commercefacades.user.data.CustomerData">
		<property name="lastLogin" type="String" />
		<property name="phoneNumber" type="String"/>

	</bean>

	<bean class="de.hybris.platform.commercefacades.user.data.RegisterData">
		<property name="phoneNumber" type="String"/>

	</bean>

	<bean class="de.hybris.training.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>

	</bean>

	<bean class="de.hybris.training.facades.forms.UpdateProfileForm">
		<property name="phoneNumber" type="String"/>
		<property name="titleCode" type="String"/>
		<property name="firstName" type="String"/>
		<property name="lastName" type="String"/>

	</bean>

	<bean class="de.hybris.training.facades.forms.EcentaRegisterForm">
		<property name="titleCode" type="String"/>
		<property name= "firstName" type="String"/>
		<property name= "lastName" type="String"/>
		<property name= "email" type="String"/>
		<property name= "pwd" type="String"/>
		<property name= "checkPwd" type="String"/>
		<!-- private ConsentForm consentForm;   DONT NEED THIS BECAUSE I CREATED MY OWN ConsentForm --> <!-- Why then this line not removed?)) -->
		<property name="termsCheck" type="Boolean"/>
		<property name= "phoneNumber" type="String"/>

	</bean>



</beans>
